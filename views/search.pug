extends layouts/main.pug

include mixins/videoBlock

block content   
    div.search__header
        h3 Searching for : #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each video in videos
           +videoBlock({
               title: video.title,
               view: video.view,
               fileUrl: video.fileUrl,
               id: video.id
           })